{
  "env":{
      "commonjs":true,
      "typescript": true,
      "es6": true,
      "node": true
  },
  "extends": ["eslint: recommand", "prettier"],
  "globals":{
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
  },
  "parserOptions":{
      "ecmaVersion":2018
  },
  "rules":{
      "no-unused-vars":"warn",
      "no-console":"off"
  }
}